<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.willkong.loadstate">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StateViewActivity"></activity>
        <activity android:name=".FragmentsActivity"></activity>
        <activity android:name="com.willkong.loadsir.MainLoadsirActivity"/>
        <activity android:name="com.willkong.loadsir.target.NormalActivity"/>
        <activity android:name="com.willkong.loadsir.target.ConvertorActivity"/>
        <activity android:name="com.willkong.loadsir.target.FragmentSingleActivity"/>
        <activity android:name="com.willkong.loadsir.target.ViewTargetActivity"/>
        <activity android:name="com.willkong.loadsir.target.DefaultCallbackActivity"/>
        <activity android:name="com.willkong.loadsir.target.MultiFragmentWithViewPagerActivity"/>
        <activity android:name="com.willkong.loadsir.target.MultiFragmentActivity"/>
        <activity android:name="com.willkong.loadsir.target.PlaceholderActivity"/>
        <activity android:name="com.willkong.loadsir.target.AnimateActivity"/>
        <activity android:name="com.willkong.loadsir.target.KeepTitleFragmentActivity"/>
        <activity android:name="com.willkong.loadsir.target.BestPracticesActivity"/>
        <activity android:name="com.willkong.loadsir.target.LottieActivity"/>
        <activity
            android:name="com.willkong.loadsir.target.KeepTitleActivity"
            android:theme="@style/NoTitle"/>
    </application>

</manifest>